# Main link to the control server
mavlink start -u _MAINPORT_ -r 4000000 -o _MAINPORTO_ -m magic
mavlink stream -r 50 -s LOCAL_POSITION_NED -u _MAINPORT_

# For debugging individual ones
mavlink start -u _DBGPORT_ -r 4000000 -m onboard -o _DBGPORTO_


sdlog2 start -r 100 -e -t -a
mavlink boot_complete
replay trystart
